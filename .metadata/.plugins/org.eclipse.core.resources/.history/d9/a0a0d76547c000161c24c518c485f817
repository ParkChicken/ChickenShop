import java.util.Scanner;


public class Helloworld {

public static void main(String[] args) {
	
	Scanner sc = new Scanner(System.in);
	int year = sc.nextInt();
	if(year % 4 == 0 )
	{
		if(year % 400 == 0){
		 System.out.println(year+"년은 윤년입니다.");
		}else if(year % 100 == 00){
			System.out.println(year+"년은 평년입니다.");
		}
		System.out.println(year+"년은 윤년입니다.");
	}else{
			System.out.println(year+"년은 평년입니다.");
		}
		
	}
}

